{
  "name": "gdal-async",
  "version": "3.1.0",
  "description": "Bindings to GDAL (Geospatial Data Abstraction Library) with full async support",
  "license": "Apache-2.0",
  "url": "https://github.com/mmomtchev/node-gdal-async",
  "main": "./lib/gdal.js",
  "types": "./lib/index.d.ts",
  "keywords": [
    "gdal",
    "libgdal",
    "ogr",
    "gis",
    "geo",
    "geos",
    "geography",
    "geospatial",
    "raster",
    "vector",
    "gtiff",
    "geojson",
    "mapping"
  ],
  "binary": {
    "module_name": "gdal",
    "module_path": "./lib/binding/{node_abi}-{platform}-{arch}",
    "remote_path": "v{version}",
    "host": "https://github.com/mmomtchev/node-gdal-async/releases/download/",
    "package_name": "{node_abi}-{platform}-{arch}.tar.gz",
    "hosting": {
      "provider": "github",
      "repo": "mmomtchev/node-gdal-async"
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/mmomtchev/node-gdal-async.git"
  },
  "author": {
    "name": "Brandon Reavis",
    "url": "https://github.com/brandonreavis",
    "email": "brandon@naturalatlas.com"
  },
  "scripts": {
    "test": "mocha test -R tap --timeout 0 --expose-gc --require ./test/_common.js --require ./test/_hooks.js",
    "clint": "clang-format -i src/*.cpp src/*.hpp && clang-format -i src/*/*.cpp src/*/*.hpp",
    "lint": "eslint lib test examples",
    "lint:fix": "eslint lib test examples --fix",
    "install": "node-pre-gyp install --fallback-to-build -j max",
    "yuidoc": "yuidoc --extension .js,.cpp,.hpp",
    "publish-yuidoc": "npm run yuidoc && node ./scripts/publish-docs.js",
    "preversion": "npm run test && npx yatag",
    "gcov": "mkdir -p coverage && cd coverage && gcov -o ../build/Debug/obj.target/gdal/src ../src/*",
    "codecov": "cd coverage && curl -s https://codecov.io/bash | bash",
    "version": "git add package.json",
    "postversion": "git push && git push --tags && node ./scripts/publish-packages.js && npm run publish-yuidoc",
    "container": "bash scripts/container.sh",
    "test:gdal1": "npm run container all 16.04 10 && npm run container all 16.04 12",
    "test:gdal2": "npm run container all 18.04 10 && npm run container all 18.04 12 && npm run container all 18.04 14",
    "test:gdal3": "npm run container all 20.04 10 && npm run container all 20.04 12 && npm run container all 20.04 14 && npm run container all 20.04 16",
    "test:all": "npm run test:gdal1 && npm run test:gdal2 && npm run test:gdal3",
    "test:fast": "npm run container all 16.04 10 && npm run container all 18.04 12 && npm run container all 20.04 16"
  },
  "dependencies": {
    "@mapbox/node-pre-gyp": "^1.0.3",
    "nan": "^2.14.0"
  },
  "bundledDependencies": [
    "@mapbox/node-pre-gyp"
  ],
  "devDependencies": {
    "@octokit/core": "^3.4.0",
    "aws-sdk": "^2.614.0",
    "chai": "^4.2.0",
    "chai-as-promised": "^7.1.1",
    "eslint": "^7.0.0",
    "eslint-plugin-array-func": "^3.0.0",
    "eslint-plugin-mocha": "^8.0.0",
    "eslint-plugin-prefer-arrow": "^1.1.3",
    "gh-pages": "^3.0.0",
    "mocha": "^8.0.0",
    "node-gyp": "^7.0.0",
    "node-pre-gyp-github": "^1.4.3",
    "segfault-handler": "^1.3.0",
    "semver": "^7.3.5",
    "yatag": "^1.0.5",
    "yuidoc-lucid-theme": "~0.1.2",
    "yuidocjs": "^0.10.2"
  },
  "engines": {
    "node": ">= 10"
  }
}
